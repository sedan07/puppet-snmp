# This file is managed by Puppet

<% if @data['custom_miboid'] -%>
################################################################################
# EXTENDING AGENT FUNCTIONALITY
<% @data['custom_miboid'].sort.each do |index,command| -%> 
extend <%= command['miboid'] %> <%= command['index_name'] %> <%= command['prog'] %> <%= command['args'] -%>
<% end -%>

################################################################################
<% end -%>

<% if @data['proc_monitor'] -%>
################################################################################
# PROCESS MONITORING
<% @data['proc_monitor'].sort.each do |index,command| -%> 
proc <%= index %> <%= command['max'] %> <%= command['min'] -%>
<% end -%>

################################################################################
<% end -%>
